#include <stdio.h>

int main() {
    int idade, matricula;
    float altura;
    char nome[50];

    printf("Digite seu nome: ");
    scanf("%s", nome);

    printf("Digite sua idade: ");
    scanf("%d", &idade);

    printf("Digite sua altura: ");
    scanf("%f", &altura);

    printf("Digite sua matricula: ");
    scanf("%d", &matricula);

    printf("Nome: %s\nIdade: %d\nAltura: %.2f\nMatricula: %d\n", nome, idade, altura, matricula);

    return 0;
}
